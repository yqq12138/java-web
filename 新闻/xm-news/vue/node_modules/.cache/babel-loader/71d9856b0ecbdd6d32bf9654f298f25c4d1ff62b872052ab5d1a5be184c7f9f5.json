{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Footer from \"@/components/Footer\";\nexport default {\n  name: \"FrontLayout\",\n  components: {\n    Footer\n  },\n  data() {\n    return {\n      top: '',\n      notice: [],\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || '{}'),\n      menus: [{\n        text: '新闻首页',\n        path: '/front/home'\n      }, {\n        text: '热门问答',\n        path: '/front/question'\n      }, {\n        text: '周边新闻',\n        path: '/front/localNews'\n      }, {\n        text: '新闻视频',\n        path: '/front/video'\n      }, {\n        text: '意见反馈',\n        path: '/front/feedback'\n      }, {\n        text: '系统公告',\n        path: '/front/notice'\n      }],\n      title: this.$route.query.title\n    };\n  },\n  mounted() {},\n  methods: {\n    search() {\n      location.href = '/front/search?title=' + this.title;\n    },\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}'); // 重新获取下用户的最新信息\n    },\n    // 退出登录\n    logout() {\n      localStorage.removeItem(\"xm-user\");\n      this.$router.push(\"/login\");\n    }\n  }\n};","map":{"version":3,"names":["Footer","name","components","data","top","notice","user","JSON","parse","localStorage","getItem","menus","text","path","title","$route","query","mounted","methods","search","location","href","updateUser","logout","removeItem","$router","push"],"sources":["src/views/Front.vue"],"sourcesContent":["<template>\r\n  <div style=\"background-color: #f8f8f8\">\r\n    <div>\r\n      <iframe scrolling=\"no\" src=\"https://widget.tianqiapi.com/?style=tg&skin=pitaya\" frameborder=\"0\" width=\"470\" height=\"40\" allowtransparency=\"true\"></iframe>\r\n    </div>\r\n    <!--头部-->\r\n    <div class=\"front-header\">\r\n      <div class=\"front-header-left\">\r\n        <img src=\"@/assets/imgs/logo1.png\" alt=\"\">\r\n        <div class=\"title\">飞扑新闻网</div>\r\n      </div>\r\n      <div class=\"front-header-center\">\r\n        <div style=\"display: flex\">\r\n          <div class=\"menu\" :class=\"{ 'menu-active' : $route.path.includes(item.path) }\"\r\n               v-for=\"item in menus\" :key=\"item.path\" @click=\"$router.push(item.path)\">{{ item.text }}</div>\r\n        </div>\r\n      </div>\r\n      <div style=\"width: 300px\">\r\n        <el-input v-model=\"title\" prefix-icon=\"el-icon-search\" size=\"medium\" placeholder=\"请输入新闻关键字搜索\" style=\"width: 220px; margin-right: 5px\"></el-input>\r\n        <el-button size=\"medium\" @click=\"search\">搜索</el-button>\r\n      </div>\r\n      <div class=\"front-header-right\">\r\n        <div v-if=\"!user.username\">\r\n          <el-button @click=\"$router.push('/login')\">登录</el-button>\r\n          <el-button @click=\"$router.push('/register')\">注册</el-button>\r\n        </div>\r\n        <div v-else>\r\n          <el-dropdown>\r\n            <div class=\"front-header-dropdown\">\r\n              <img :src=\"user.avatar\" alt=\"\">\r\n              <div style=\"margin-left: 10px\">\r\n                <span>{{ user.name }}</span><i class=\"el-icon-arrow-down\" style=\"margin-left: 5px\"></i>\r\n              </div>\r\n            </div>\r\n            <el-dropdown-menu slot=\"dropdown\">\r\n              <el-dropdown-item>\r\n                <div @click=\"$router.push('/front/userNews')\">我的新闻</div>\r\n              </el-dropdown-item>\r\n              <el-dropdown-item>\r\n                <div @click=\"$router.push('/front/userQuestion')\">我的问题</div>\r\n              </el-dropdown-item>\r\n              <el-dropdown-item>\r\n                <div @click=\"$router.push('/front/userAnswer')\">我的回答</div>\r\n              </el-dropdown-item>\r\n              <el-dropdown-item>\r\n                <div @click=\"$router.push('/front/userFeedback')\">我的反馈</div>\r\n              </el-dropdown-item>\r\n              <el-dropdown-item>\r\n                <div @click=\"$router.push('/front/person')\">个人信息</div>\r\n              </el-dropdown-item>\r\n              <el-dropdown-item>\r\n                <div @click=\"logout\">退出登录</div>\r\n              </el-dropdown-item>\r\n            </el-dropdown-menu>\r\n          </el-dropdown>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!--主体-->\r\n    <div class=\"main-body\">\r\n      <router-view ref=\"child\" @update:user=\"updateUser\" />\r\n    </div>\r\n\r\n    <Footer />\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport Footer from \"@/components/Footer\";\r\nexport default {\r\n  name: \"FrontLayout\",\r\n  components: {\r\n    Footer\r\n  },\r\n  data () {\r\n    return {\r\n      top: '',\r\n      notice: [],\r\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || '{}'),\r\n      menus: [\r\n        { text: '新闻首页', path: '/front/home' },\r\n        { text: '热门问答', path: '/front/question' },\r\n        { text: '周边新闻', path: '/front/localNews' },\r\n        { text: '新闻视频', path: '/front/video' },\r\n        { text: '意见反馈', path: '/front/feedback' },\r\n        { text: '系统公告', path: '/front/notice' },\r\n      ],\r\n      title: this.$route.query.title\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n\r\n  },\r\n  methods: {\r\n    search() {\r\n      location.href = '/front/search?title=' + this.title\r\n    },\r\n    updateUser() {\r\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}')   // 重新获取下用户的最新信息\r\n    },\r\n    // 退出登录\r\n    logout() {\r\n      localStorage.removeItem(\"xm-user\");\r\n      this.$router.push(\"/login\");\r\n    },\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  @import \"@/assets/css/front.css\";\r\n\r\n  .menu {\r\n    padding: 0 20px;\r\n    color: #0551b6;\r\n    cursor: pointer;\r\n    font-size: 16px;\r\n  }\r\n  .menu:hover:not(.menu-active) {\r\n    color: #fff;\r\n  }\r\n  .menu-active {\r\n    color: #fff;\r\n    background-color: #2a60c9;\r\n  }\r\n</style>"],"mappings":";AAsEA,OAAAA,MAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAG,KAAA;IACA;MACAC,GAAA;MACAC,MAAA;MACAC,IA