{"ast":null,"code":"export default {\n  name: \"Question\",\n  data() {\n    return {\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      noAnswerList: []\n    };\n  },\n  created() {\n    this.load(1);\n    this.loadNoAnswer();\n  },\n  methods: {\n    loadNoAnswer() {\n      this.$request.get('/question/selectNoAnswer').then(res => {\n        this.noAnswerList = res.data || [];\n      });\n    },\n    load(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/question/selectFrontPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data?.list;\n          this.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","noAnswerList","created","load","loadNoAnswer","methods","$request","get","then","res","params","code","list","$message","error","msg","handleCurrentChange"],"sources":["src/views/front/Question.vue"],"sourcesContent":["<template>\r\n  <div style=\"width: 60%; margin: 10px auto\">\r\n    <div style=\"display: flex; align-items: flex-start; grid-gap: 15px\">\r\n      <div style=\"flex: 1\" class=\"card\">\r\n        <div @click=\"$router.push('/front/questionDetail?id=' + item.id)\" v-for=\"(item, index) in tableData\" :key=\"item.id\" class=\"item\">\r\n          <div style=\"width: 20px; padding-top: 10px\">\r\n            <div v-if=\"index < 3\" style=\"width: 10px; height: 10px; background-color: orangered; border-radius: 50%\"></div>\r\n            <div v-else style=\"width: 10px; height: 10px; background-color: #2a60c9; border-radius: 50%\"></div>\r\n          </div>\r\n          <div style=\"flex: 1; width: 0\">\r\n            <div style=\"font-size: 20px; margin-bottom: 10px\" class=\"title line2\">{{ item.title }}</div>\r\n            <div style=\"color: #666\" class=\"line2\">{{ item.title }}</div>\r\n          </div>\r\n          <div>\r\n            <img :src=\"item.img\" alt=\"\" style=\"width: 150px; height: 100px; border-radius: 5px\">\r\n          </div>\r\n        </div>\r\n\r\n        <div style=\"margin: 20px 0\">\r\n          <el-pagination\r\n              background\r\n              @current-change=\"handleCurrentChange\"\r\n              :current-page=\"pageNum\"\r\n              :page-sizes=\"[5, 10, 20]\"\r\n              :page-size=\"pageSize\"\r\n              layout=\"total, prev, pager, next\"\r\n              :total=\"total\">\r\n          </el-pagination>\r\n        </div>\r\n      </div>\r\n\r\n      <div style=\"width: 300px\">\r\n        <div style=\"display: flex; margin-bottom: 10px\">\r\n          <img src=\"@/assets/imgs/问.png\" alt=\"\" style=\"width: 20px; height: 20px; margin-top: 5px; margin-right: 5px\">\r\n          <span style=\"font-size: 20px\">等你回答</span>\r\n        </div>\r\n        <div>\r\n          <div class=\"card\" v-for=\"item in noAnswerList\" :key=\"item.id\" style=\"margin-bottom: 10px\">\r\n            <div style=\"margin-bottom: 10px; font-size: 16px\">{{ item.title }}</div>\r\n            <div style=\"color: #888; display: flex\">\r\n              <div style=\"flex: 1\">{{ item.date }}</div>\r\n              <el-button @click=\"$router.push('/front/questionDetail?id=' + item.id)\" type=\"primary\" size=\"mini\" icon=\"el-icon-edit\">写回答</el-button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Question\",\r\n  data() {\r\n    return {\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      noAnswerList: []\r\n    }\r\n  },\r\n  created() {\r\n    this.load(1)\r\n    this.loadNoAnswer()\r\n  },\r\n  methods: {\r\n    loadNoAnswer() {\r\n      this.$request.get('/question/selectNoAnswer').then(res => {\r\n        this.noAnswerList = res.data || []\r\n      })\r\n    },\r\n    load(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/question/selectFrontPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n        }\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.tableData = res.data?.list\r\n          this.total = res.data?.total\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.item {\r\n  display: flex;\r\n  margin-bottom: 15px;\r\n  cursor: pointer;\r\n}\r\n.item:hover .title {\r\n  color: orangered;\r\n}\r\n</style>"],"mappings":"AAmDA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,YAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;IACA,KAAAC,YAAA;EACA;EACAC,OAAA;IACAD,aAAA;MACA,KAAAE,QAAA,CAAAC,GAAA,6BAAAC,IAAA,CAAAC,GAAA;QACA,KAAAR,YAAA,GAAAQ,GAAA,CAAAb,IAAA;MACA;IACA;IACAO,KAAAL,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAQ,QAAA,CAAAC,GAAA;QACAG,MAAA;UACAZ,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAS,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAE,IAAA;UACA,KAAAd,SAAA,GAAAY,GAAA,CAAAb,IAAA,EAAAgB,IAAA;UACA,KAAAZ,KAAA,GAAAS,GAAA,CAAAb,IAAA,EAAAI,KAAA;QACA;UACA,KAAAa,QAAA,CAAAC,KAAA,CAAAL,GAAA,CAAAM,GAAA;QACA;MACA;IACA;IACAC,oBAAAlB,OAAA;MACA,KAAAK,IAAA,CAAAL,OAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}